{
  "name": "robby",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "rebuild": "npm rebuild robotjs --runtime=electron --target=6.0.12 --disturl=https://atom.io/download/atom-shell --abi=72"
  },
  "main": "background.js",
  "dependencies": {
    "axios": "^0.19.1",
    "clipboardy": "file:./src/plugins/clipboardy",
    "core-js": "^3.4.4",
    "element-ui": "^2.13.0",
    "iohook": "^0.6.4",
    "node-xlsx": "^0.15.0",
    "robotjs": "^0.6.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-plugin-vuex": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "electron": "^15.5.5",
    "electron-devtools-installer": "^2.2.4",
    "node-sass": "^4.9.2",
    "sass-loader": "^7.0.3",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-axios": "0.0.4",
    "vue-cli-plugin-electron-builder": "^1.4.4",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "^2.6.10"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "iohook": {
    "targets": [
      "node-72",
      "electron-73"
    ],
    "platforms": [
      "win32"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  }
}
